<div class="navbar">
    <a href="/">Home</a>
    <a href="/collections">Collections</a>
    <a href="/profile">My Profile</a>
</div>
<div class="body">
    <h1>Feed</h1>
    {{#if error}}
    <p class="error">{{error}}</p>
    {{/if}}
    <div class = "clientError" id = "clientError"></div>
    <a href="/createpost">Create Post</a>
    <ul>
    {{#each posts}}
        <article class="article">
            <h2>{{title}}</h2>
            <p>{{file}}</p>
            <p>{{caption}}</p>
        </article>
    {{/each}}
    </ul>
</div>